using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace lab_1_q1_part_2
{
    class Program
    {
        static void Main(string[] args)
        {
            int sum = 0;
            for (int i = 40; i <= 60; i++)
            {
                if (i == 46 && i == 48)
                    continue;
                sum += i;
            }
            Console.WriteLine(sum);
            Console.ReadLine();
        }  
    }
}
